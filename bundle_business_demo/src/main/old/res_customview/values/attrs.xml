        </attr>
        <attr name="demoImageWidth" format="dimension">
        </attr>
        <attr name="demoImageHeight" format="dimension">
        </attr>
        <attr name="demoImageTextSize" format="dimension">
        </attr>
        <attr name="demoImageTextColor" format="color">
        </attr>
    </declare-styleable>
</resources>
